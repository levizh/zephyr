&i2c1 {
	status = "okay";
	clock-frequency = < 100000 >;
	interrupts = < INT010_IRQn HC32_I2C1_EE_IRQ_PRIO INT_SRC_I2C1_EEI>,
				< INT011_IRQn HC32_I2C1_TE_IRQ_PRIO INT_SRC_I2C1_TEI>,
				< INT012_IRQn HC32_I2C1_TX_IRQ_PRIO INT_SRC_I2C1_TXI>,
				< INT013_IRQn HC32_I2C1_RX_IRQ_PRIO INT_SRC_I2C1_RXI>;

	eeprom@54 {
		compatible = "atmel,at24";
		reg = <0x54>;
		label = "EEPROM_SLAVE_0";
		size = <1>;
	};
};


&i2c3 {
	status = "okay";
	clock-frequency = < 100000 >;

	eeprom@56 {
		compatible = "atmel,at24";
		reg = <0x56>;
		label = "EEPROM_SLAVE_1";
		size = <1>;
	};
};
